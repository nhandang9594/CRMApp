<!--index.cshtml is a razor view...
    razor is a viewengine, it will convert the C# code to 
    the script code which a browser can understand
-->
@model IEnumerable<ProductResponseModel>
<a asp-action="Create" asp-controller="Product" class="bg-success btn">Add Product</a>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>SupplierId</th>
            <th>CategoryId</th>
            <th>QuantityPerUnit</th>
            <th>UnitPrice</th>
            <th>UnitsInStock</th>
            <th>UnitsOnOrder</th>
            <th>ReorderLevel</th>
            <th>Discontinued</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach(var p in Model)
            {
               <partial name="_ProductRow" model="@p"></partial>
            }
        }
    </tbody>
</table>