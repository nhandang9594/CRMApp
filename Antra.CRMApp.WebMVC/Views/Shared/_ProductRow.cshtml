@model ProductResponseModel
<tr>
    <td>@Model.Id</td>
    <td>@Model.Name</td>
    <td>@Model.VendorId</td>
    <td>@Model.CategoryId</td>
    <td>@Model.QuantityPerUnit</td>
    <td>@Model.UnitPrice</td>
    <td>@Model.UnitsInStock</td>
    <td>@Model.UnitsOnOrder</td>
    <td>@Model.ReorderLevel</td>
    <td>
        @{
            var discontinuedStatus = "Available";
            if(@Model.Discontinued)
            {
                discontinuedStatus = "Discontinued";
            }
        }
        @discontinuedStatus
    </td>
    <td>
        <span>
            <a asp-controller="Product" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
        </span>
    </td>
    <td>
        <span>
            <a asp-controller="Product" asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Delete</a>
        </span>
    </td>
</tr> 