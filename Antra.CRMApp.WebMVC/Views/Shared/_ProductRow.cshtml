@model ProductResponseModel
<tr>
    <td>@Model.Id</td>
    <td>@Model.Name</td>
    <td>@Model.VendorId</td>
    <td>@Model.CategoryId</td>
    <td>@Model.QuantityPerUnit</td>
    <td>@Model.UnitPrice</td>
    <td>@Model.UnitsInStock</td>
    <td>@Model.UnitsOnOrder</td>
    <td>@Model.ReorderLevel</td>
    <td>@Model.Discontinued</td>
    <td>
        <span>
            <a asp-controller="Product" asp-action="Edit" asp-route-id="@Model.Id" >Edit</a>
        </span>
    </td>
    <td>
        <span>
            <a asp-controller="Product" asp-action="Delete" asp-route-id="@Model.Id" class="text-danger">Delete</a>
        </span>
    </td>
</tr> 